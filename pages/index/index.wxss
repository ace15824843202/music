/* pages/request/request.wxss */
.title{
  font-size: 80rpx;
  line-height: 120rpx;
  color:rgb(224, 175, 39);
  text-align: center;
}

input{
  border:2rpx solid #ccc;
  height:80rpx;
  width: 90%;
  padding-left: 15rpx;
  margin: 20rpx auto;
}
button{
  width: 90%;
}
view navigator{
  border: 4rpx solid #333;
  line-height: 60rpx;
  margin: 50rpx 30rpx;
  padding: 10rpx 0;
  background: rgba(123, 168, 204, 0.671);
  border-radius: 10rpx;
   
}
.clearfix::after{
  content: "";
  clear:both;
  display: block;
}
view navigator image{
  margin-left: 10rpx;
  width: 90px; 
  height: 90px;
  border-radius:50%;
  float:left;
}
.intro{
    margin: 40rpx 30rpx;
   
}
.int{
  display: block;
  overflow: hidden;    
  white-space:nowrap;
  text-overflow: ellipsis;
  padding-left: 20rpx;
   

}

.tipSty{
  text-align: center;
  margin: 200rpx auto;
}